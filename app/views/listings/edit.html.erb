<h1>Listings#edit</h1>
<p>Find me in app/views/listings/edit.html.erb</p>

<%= form_for @listing, html: { multipark: true } do |f| %>
Title: <%= f.text_field :title %><br>
Body: <%= f.text_area :body %><br>
Photo: <%= f.file_field :photo %>
  <%= f.submit 'Edit', class: 'btn btn-success' %>
<% end %>
