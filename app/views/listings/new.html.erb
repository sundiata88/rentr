<h1>Listings#new</h1>
<p>Find me in app/views/listings/new.html.erb</p>

<%= form_for @listing, html: { multipart: true } do |f| %>
Photo: <%= f.file_field :photo %><br>
Title: <%= f.text_field :title %><br>
Body: <%= f.text_area :body %><br>
  <%= f.submit 'Create Listing', class: 'btn btn-success' %>
<% end %>


<form>
  <%= form_for @listing, html: { multipart: true } do |f| %>
  <div class="form-group">
    <label for="exampleInputTitle">Title: <%= f.text_field :title %></label>
    <input type="title" class="form-control" id="exampleInputTitle" placeholder="Title">
  </div>
  <div class="form-group">
    <label for="exampleInputBody">Description: <%= f.text_area :body %></label>
    <input type="description" class="form-control" id="exampleInputdescription" placeholder="Describe here">
  </div>
  <div class="form-group">
    <label for="exampleInputFile">Add a photo! <%= f.file_field :photo %></label>
    <input type="file" id="exampleInputFile"><br>
  </div>
  <button type="submit" class="btn btn-success">Create listing <%= f.submit 'Create Listing', class: 'btn btn-success' %></button>
</form>

<% end %>
