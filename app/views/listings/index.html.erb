<h1>Listings#index</h1>
<p>Find me in app/views/listings/index.html.erb</p>

<ul class="nav nav-pills">
  <div class="form-group">
      <div class="col-md-2 col-md-offset-10">
          <%= link_to "Create Listing", new_listing_path, class: 'btn btn-primary' %>
      </div>
  </div>


  <div class="form-group">
      <div class="col-md-2 col-md-offset-10">
          <%= link_to "Logout", root_path, class: 'btn btn-primary' %>
      </div>
  </div>
</ul>
<hr>


<div class="container">
<% @listings.each_slice(3).each do |group| %>
  <div class="row">
    <% group.each do |u| %>
      <div class="col-md-6 col-md-4">
        <div class="caption">
          <h3><%= u.title %><br></h3>
        <p><%= link_to image_tag(u.photo.url(:medium)), listing_path(u) %>
        <%= u.body %><br></p>
              <p><%= link_to "Edit Listing", edit_listing_path(u), class: 'btn btn-primary' %></p>
            </div>
        </div>
    </div>
  <% end %>
  </div>
<% end %>
</div>
